GET     /                           controllers.Mergical.index
GET     /sign-in                    controllers.Mergical.signin
GET     /sign-in-callback           controllers.Mergical.signinCallback
GET     /sign-out                   controllers.Mergical.signOut
GET     /dashboard                  controllers.Mergical.dashboard
GET     /dashboard/new-feed         controllers.Mergical.addGeneratorForm(name: String)
POST    /add                        controllers.Mergical.addFeed(name, url)
DELETE  /remove                     controllers.Mergical.removeFeed(id: Int)
POST    /generator                  controllers.Mergical.addGenerator
DELETE  /generator                  controllers.Mergical.removeGenerator(id: Int)

GET     /assets/js/routes.js        controllers.Mergical.jsRoutes
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET     /:id                        controllers.Mergical.generator(id: Int)
